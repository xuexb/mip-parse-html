{
  "name": "mip-parse-html",
  "version": "0.0.1",
  "description": "解析 HTML 代码为 MIP-HTML",
  "main": "lib/index.js",
  "scripts": {
    "commitmsg": "validate-commit-msg",
    "test:watch": "npm run test -- --watch",
    "test:cov": "istanbul cover node_modules/mocha/bin/_mocha -- -t 5000 --recursive  -R spec test/",
    "test": "mocha --reporter spec --timeout 5000 --recursive test/"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "test",
        "chore",
        "revert",
        "release"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuexb/mip-parse-html.git"
  },
  "keywords": [
    "MIP",
    "MIP-HTML"
  ],
  "author": "xuexb <fe.xiaowu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xuexb/mip-parse-html/issues"
  },
  "homepage": "https://github.com/xuexb/mip-parse-html#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.4",
    "mock-require": "^3.0.1",
    "validate-commit-msg": "^2.14.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "crypto-random-string": "^1.0.0",
    "html-minifier": "^3.5.11",
    "htmlcs": "^0.4.1",
    "mip-validator": "^1.5.15",
    "require-dir": "^1.0.0"
  },
  "engines": {
    "node": ">= 4.0.0"
  }
}
